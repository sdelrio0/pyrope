{
  "version": "0.0.0-semantically-released",
  "name": "pyrope",
  "description": "Pyrope is a Node.js promise-based ORM for GraphQL and AWS DynamoDB.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist lib",
    "test": "mocha --compilers js:babel-core/register --require ./__tests__/test_helper.js --recursive ./__tests__/tests",
    "test:watch": "npm run test -- -w",
    "cover": "nyc --reporter=lcov npm run test",
    "cover:report": "npm run cover && nyc report",
    "cover:report:check": "npm run cover:report && npm run check-coverage",
    "check-coverage": "nyc check-coverage --statements 10 --branches 10 --functions 10 --lines 10",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz",
    "dynamo": "dynalite --port 8000 --path .dynamodb --createTableMs 0 --deleteTableMs 0 --updateTableMs 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sdelrio0/pyrope.git"
  },
  "keywords": [
    "aws",
    "dynamodb",
    "dynamo",
    "db",
    "graphql",
    "promise",
    "model",
    "schema",
    "orm"
  ],
  "author": "Sergio <sdelrio0@gmail.com> (http://xilix.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sdelrio0/pyrope/issues"
  },
  "homepage": "https://github.com/sdelrio0/pyrope#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "graphql": "^0.6.2",
    "jsonwebtoken": "^7.1.9",
    "underscore": "^1.8.3",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "aws-sdk": "^2.5.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.8.5",
    "dynalite": "^1.0.3",
    "esdoc-es7-plugin": "0.0.3",
    "ghooks": "^1.3.2",
    "graphql": "^0.6.2",
    "mocha": "^3.0.2",
    "nyc": "^7.1.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "_ghooks": {
      "pre-push": "npm run cover && npm run check-coverage"
    }
  }
}
